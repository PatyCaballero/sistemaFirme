<p id="notice"><%= notice %></p>
<div id="main-container" class="container">
      <div class="row"><br><br>
      <div class="col-xs-12">
          <div class="col-xs-6">
          </div>

          <div class="col-xs-6">
          		<%= link_to edit_stocks_path(@stocks), class: "btn btn-info" do %><i class="glyphicon glyphicon-pencil"></i> Editar <% end %>  |
				<%= link_to stock_path, class: "btn btn-default"  do %><i class="glyphicon glyphicon-arrow-left"></i> Atras <% end %>
		</div>
          <br><br><br><br>
          <div class="col-xs-10">
            <table class="table table-striped">
              <tbody>
                <tr>

					<td><p>
					  <strong>Producto:</strong>
					  <%= @stocks.producto.nomProducto %>
					</p></td>

					<td><p>
					  <strong>Cantidad:</strong>
					  <%= @stocks.cantidad %>
					</p></td>

					<td><p>
					  <strong>Descripcion:</strong>
					  <%= @stocks.descripcion %>
					</p></td>
				</tr>
              </tbody>
            </table>          
          </div>
      </div>
    </div>
  </div>

