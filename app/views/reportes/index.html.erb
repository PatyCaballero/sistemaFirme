<p id="notice"><%= notice %></p>

<h1>Reportes</h1>

<table>
  <thead>
    <tr>
      <th>Horaentrada</th>
      <th>Horasalida</th>
      <th>Estado</th>
      <th>Novedad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reportes.each do |reporte| %>
      <tr>
        <td><%= reporte.horaEntrada %></td>
        <td><%= reporte.horaSalida %></td>
        <td><%= reporte.estado %></td>
        <td><%= reporte.novedad %></td>
        <td><%= link_to 'Show', reporte %></td>
        <td><%= link_to 'Edit', edit_reporte_path(reporte) %></td>
        <td><%= link_to 'Destroy', reporte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reporte', new_reporte_path %>
